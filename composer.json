{
    "name": "karser/custom-installer",
    "type": "composer-installer",
    "description": "This installer for composer packages allows you to specify custom install path per package in Symfony2",
    "keywords": ["custom installer"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "composer/installers": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Karser\\CustomInstaller": "" }
    },
    "target-dir": "Karser/CustomInstaller",
    "authors": [
        {
            "name": "Dmitry Poddubny",
            "email": "dpoddubny@gmail.com"
        }
    ],
    "extra": {
        "class": "Karser\\CustomInstaller\\Installer"
    }
}
